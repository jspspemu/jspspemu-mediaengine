# https://github.com/kripken/emscripten/blob/master/src/settings.js
cp ffmpeg/ffmpeg ffmpeg/ffmpeg.bc
#	-s TOTAL_MEMORY=8000000 -O2 --memory-init-file 0 \
emcc -s NO_EXIT_RUNTIME=1 -s OUTLINING_LIMIT=100000 \
	-s EXPORTED_FUNCTIONS="['_main', '_main2']" \
	-s TOTAL_MEMORY=8000000 -O2 --memory-init-file 1 \
	ffmpeg/ffmpeg.bc main.bc --pre-js pre.js --post-js post.js \
	-o ./jspspemu-me.js
